			i3c0fclksdiv: i3c0fclksdiv@4000053c {
				compatible = "nxp,syscon-clock-div";
				#clock-cells = <1>;
				/* SYSCON0::I3C0FCLKSDIV[DIV] */
				reg = <0x4000053c 0x8>;

				i3c0_slow_clock: i3c0-slow-clock {
					compatible = "clock-output";
					#clock-cells = <1>;
