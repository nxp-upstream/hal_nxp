	#size-cells = <1>;

	/* Root clock sources */
	no_clock: no-clock {
		/* Dummy node- indicates no clock source was selected */
		compatible = "fixed-clock-source";
		#clock-cells = <0>;
		frequency = <0>;
	};

	sosc: sosc@40044100 {
		compatible = "nxp,syscon-clock-source";
		offset = <0x0>;
		#clock-cells = <1>;
		/* SOSCCSR[SOSCEN] */
		reg = <0x40044100 0x1>;
